{
    "description": "",
    "manifest_version": 2,
    "name": "HackBar",
    "version": "0.0.1",
    "icons": {
        "48": "icons/icon.png"
    },
    "author": "whoami",
    "homepage_url": "https://github.com/Buzz2d0/HackBar",
    "permissions": [
        "tabs",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ],
    "devtools_page": "hackbar.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}